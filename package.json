  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:pull:dev": "dotenv -e .env.development.local -- yarn prisma db pull",
    "prisma:push:dev": "dotenv -e .env.development.local -- yarn prisma db push",
