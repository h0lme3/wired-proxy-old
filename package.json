    "discord.js": "^13.6.0",
    "jquery": "^3.6.0",
    "json2csv": "^5.0.7",
    "micro": "^9.3.4",
    "next": "12.1.0",
    "next-auth": "^4.3.1",
    "next-mdx-remote": "^5.0.0",
    "nodemailer": "^6.7.3",
    "nprogress": "^0.2.0",
